{
	"name": "ghost-miniprogram",
	"version": "0.0.1",
	"private": true,
	"description": "Rapidly develop templates based on tarojs.",
	"templateInfo": {
		"name": "vue3-pinia",
		"typescript": true,
		"css": "less"
	},
	"scripts": {
		"lint": "eslint src --ext .ts,.tsx",
		"build:weapp": "npm run lint && taro build --type weapp",
		"build:alipay": "npm run lint && taro build --type alipay",
		"build:tt": "npm run lint && taro build --type tt",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"debug": "npm run dev:weapp -- --debug",
		"mock": "npm run dev:weapp -- --mock"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "yangtianxia",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
